Services = setmetatable({}, {
	__index = function(self, name)
		local success, cache = pcall(function()
			return cloneref(game:GetService(name))
		end)
		if success then
			rawset(self, name, cache)
			return cache
		else
			error("Invalid Service: " .. tostring(name))
		end
	end
})
HttpService = Services.HttpService
PlaceId = game.PlaceId
JobId = game.JobId
Players = Services.Players
TeleportService = Services.TeleportService
function serverhop()
	local servers = {}
	local req = game:HttpGet("https://games.roblox.com/v1/games/" .. PlaceId .. "/servers/Public?excludeFullGames=true&limit=100")
	local body = HttpService:JSONDecode(req)
	if body and body.data then
		for i, v in next, body.data do
			if type(v) == "table" and tonumber(v.playing) and tonumber(v.maxPlayers) and v.playing < v.maxPlayers and v.id ~= JobId then
				table.insert(servers, 1, v.id)
			end
		end
	end
	if #servers > 0 then
		TeleportService:TeleportToPlaceInstance(PlaceId, servers[math.random(1, #servers)], Players.LocalPlayer)
	else
		warn("Serverhop", "Couldn't find a server.")
	end
end
repeat task.wait() until game:IsLoaded()
function printcurrentbank()
	repeat task.wait() until workspace:WaitForChild("Map"):WaitForChild("MapModel"):WaitForChild("Bank"):WaitForChild("ScriptedElements"):WaitForChild("EntranceDoor"):WaitForChild("Shutter")
	local Map = workspace.Map
	local Bank = Map.MapModel.Bank
	local EntranceDoor = Bank.ScriptedElements.EntranceDoor
    local Shutter = EntranceDoor.Shutter
	if Shutter.Transparency ~= 1 then
    	print("Closed")
    	return false
	else
    	print("Open")
    	return true
	end
end
printcurrentbank()
function searchopenbank()
	if not printcurrentbank() then
		serverhop()
	end
end
searchopenbank()
getgenv().searchopenbank = searchopenbank
getgenv().printcurrentbank = printcurrentbank
Players.LocalPlayer.OnTeleport:Connect(function(State)
	if queueonteleport then
		queueonteleport([[
			repeat task.wait() until game:IsLoaded()
			printcurrentbank()
			searchopenbank()
		]])
	else
		error('Incompatible Exploit. Your exploit does not support this command (missing queueonteleport)')
	end
end)
